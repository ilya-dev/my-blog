{
	"name": "ilya/my-blog",
	"description": "The source code of my blog.",
	"license": "MIT",
	"require": {
		"laravel/framework": "4.1.*"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds"
		],
		"psr-4": {
            "Blog\\": "app/Blog/"
        }
	},
	"require-dev": {
		"phpspec/phpspec": "~2",
		"behat/behat": "~2.5",
		"behat/mink": "~1.5",
		"behat/mink-goutte-driver": "*"
	}
}
